<tool id="bionlp.proteins.get" name="Get Document" version="2.0.0">
 <description>
  Retrieves a single document from the BioNLP Protein corpus.
 </description>
 <command>
  $__tool_directory__/get.lsd $key $type $output
 </command>
 <inputs>
  <param label="Key" name="key" type="text" value="PMC-3589482-6-Experimental_Procedures.lif">
  </param>
  <param name="type" type="select">
  	<option value="lif">Annotated</option>
  	<option value="txt">Text only</option>
  </param>
 </inputs>
 <outputs>
  <data format="lif" label="BioNLP Reference Document" name="output">
  </data>
 </outputs>
 <help>
  <![CDATA[  

*TODO*: this.

]]>
 </help>
</tool>
