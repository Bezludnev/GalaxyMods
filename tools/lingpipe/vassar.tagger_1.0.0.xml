<tool id="vassar.lingpipe.tagger" name="Lingpipe Tagger" version="1.0.0">
 <description>
  (Vassar)
 </description>
 <command>lsd $__tool_directory__/invoke.lsd tagger $version $input $output</command>
 <inputs>
  <param format="lif" label="input" name="input" type="data">
  </param>
